  <owl:Class rdf:about="https://w3id.org/apn/AccountManagement">
    <rdfs:label>Account Management</rdfs:label>
    <skos:definition xml:lang="en">Notifications that encourage subscribers to create and/or log into an account to take advantage of a certain feature.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/ArtAndDesign">
    <rdfs:label>Art And Design</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Awareness">
    <rdfs:label>Awareness</rdfs:label>
    <skos:definition xml:lang="en">The notification makes subscribers aware of information that does not need further investigation by the subscriber, e.g., a new feature, upcoming holiday, weather, stock price, score in a match, etc. </skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Beauty">
    <rdfs:label>Beauty</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/BooksAndReference">
    <rdfs:label>Books And Reference</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Business">
    <rdfs:label>Business</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Awareness">
    <rdfs:label>Campaign Type: Awareness</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote general awareness of a topic.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_BreakingContent">
    <rdfs:label>Campaign Type: Breaking Content</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote new content that has just been created.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_HelpFeedbackTips">
    <rdfs:label>Campaign Type: Help Feedback Tips</rdfs:label>
    <skos:definition xml:lang="en">A campaign to get help or tips and to provide feedback.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Loyalty">
    <rdfs:label>Campaign Type: Loyalty</rdfs:label>
    <skos:definition xml:lang="en">A campaign directed at loyal / returning / engaged users or customers.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Newsletter">
    <rdfs:label>Campaign Type: Newsletter</rdfs:label>
    <skos:definition xml:lang="en">Informative campaign containing 'things of interest' to the user.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Onboarding">
    <rdfs:label>Campaign Type: Onboarding</rdfs:label>
    <skos:definition xml:lang="en">A campaign to induct users with a product/service, to acquaint them with features available or account management.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Product">
    <rdfs:label>Campaign Type: Product</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote a particular product/feature.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_PromotionalFlash">
    <rdfs:label>Campaign Type: Promotional Flash</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote flash sale offers, i.e., they have some time limit associated.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CT_Promotional"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_PromotionalGeneral">
    <rdfs:label>Campaign Type: Promotional General</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote general offers for the users.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CT_Promotional"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_PromotionalRare">
    <rdfs:label>Campaign Type: Promotional Rare</rdfs:label>
    <skos:definition xml:lang="en">A campaign to promote a service/product associated with particular seasons and/or holidays/events.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CT_Promotional"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Reengagement">
    <rdfs:label>Campaign Type: Reengagement</rdfs:label>
    <skos:definition xml:lang="en">A campaign enticing users / customers to re-engage with the service.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Social">
    <rdfs:label>Campaign Type: Social</rdfs:label>
    <skos:definition xml:lang="en">A campaign based on or associated with social tools, social media or influencers.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Claim">
    <rdfs:label>Claim</rdfs:label>
    <skos:definition xml:lang="en">Notifications that encourage the subscribers to click and claim some prize or reward from the application.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Comics">
    <rdfs:label>Comics</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Communication">
    <rdfs:label>Communication</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Country">
    <rdfs:label>Country</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their country of residence.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Geographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Discover">
    <rdfs:label>Discover</rdfs:label>
    <skos:definition xml:lang="en">Notifications that tease the content of the information itself to generate curiosity on the users and lead them to engage with the app.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Download">
    <rdfs:label>Download</rdfs:label>
    <skos:definition xml:lang="en">Notifications to get the subscriber to download some asset.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Education">
    <rdfs:label>Education</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Engagement">
    <rdfs:label>Engagement</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their engagement with the app, e.g., they have liked a video, followed another user, started a game.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Behavior"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Entertainment">
    <rdfs:label>Entertainment</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Events">
    <rdfs:label>Events</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/FamilyStatus">
    <rdfs:label>Family Status</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their family status, e.g., new parents, pregnancies.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Demographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Feedback">
    <rdfs:label>Feedback</rdfs:label>
    <skos:definition xml:lang="en">Notifications to get the subscriber to provide some form of feedback or response, for instance on the app's functionalities.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Finance">
    <rdfs:label>Finance</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/FoodAndDrink">
    <rdfs:label>Food And Drink</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Games">
    <rdfs:label>Games</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Health">
    <rdfs:label>Health</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their health status.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Demographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/HealthAndFitness">
    <rdfs:label>Health And Fitness</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Holidays">
    <rdfs:label>Holidays</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant in the time leading up to a holiday (Christmas, Halloween, Mother's Day, ...).</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Interest">
    <rdfs:label>Interest</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their interests, personal motivations or lifestyle.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Psychographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Job">
    <rdfs:label>Job</rdfs:label>
    <skos:definition xml:lang="en">Targeting users who have a particular interest, e.g., football goal updates indicates an interest in sports and a new recipe an interest in cooking.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Demographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/LibrariesAndDemo">
    <rdfs:label>Libraries And Demo</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Lifestyle">
    <rdfs:label>Lifestyle</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their particular lifestyle, e.g., vegan products, a person who runs or works out.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Psychographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Loyalty">
    <rdfs:label>Loyalty</rdfs:label>
    <skos:definition xml:lang="en">Targeting a segment of users who are particularly loyal, e.g., daily recurrent users.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Behavior"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/MapsAndNavigation">
    <rdfs:label>Maps And Navigation</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Medical">
    <rdfs:label>Medical</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Motivation">
    <rdfs:label>Motivation</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their aspirations or motivations, e.g., find a new job, get fitter, learn a new skill.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Psychographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/MusicAndAudio">
    <rdfs:label>Music And Audio</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NewsAndMagazines">
    <rdfs:label>News And Magazines</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextDays">
    <rdfs:label>Next Days</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant for the next few days.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextHours">
    <rdfs:label>Next Hours</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant for the next few hours.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextMinutes">
    <rdfs:label>Next Minutes</rdfs:label>
    <skos:definition xml:lang="en">The notification will only be relevant for the next few minutes.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextMonths">
    <rdfs:label>Next Months</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant for the next few months.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextWeeks">
    <rdfs:label>Next Weeks</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant for the next few weeks.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/NextYears">
    <rdfs:label>Next Years</rdfs:label>
    <skos:definition xml:lang="en">The notification will be relevant for the next few years.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Occasion">
    <rdfs:label>Occasion</rdfs:label>
    <skos:definition xml:lang="en">Targeting users who have engaged with or expressed interest in rare events, e.g., previously purchased during a holiday.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Behavior"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/PersonalMotivations">
    <rdfs:label>Personal Motivations</rdfs:label>
    <skos:definition xml:lang="en">Information about personal motivations.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Personalisation"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Photography">
    <rdfs:label>Photography</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Play">
    <rdfs:label>Play</rdfs:label>
    <skos:definition xml:lang="en">Notifications related with gaming apps, where the subscriber is encouraged to re-engage with the game.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Product">
    <rdfs:label>Product</rdfs:label>
    <skos:definition xml:lang="en">Targeting users who have engaged with or expressed interest in a specific product, e.g., you will love the new X product just launched.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Behavior"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Productivity">
    <rdfs:label>Productivity</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Purchase">
    <rdfs:label>Purchase</rdfs:label>
    <skos:definition xml:lang="en">Notifications to get the subscriber to purchase a product or service.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/PurchasingReason">
    <rdfs:label>Purchasing Reason</rdfs:label>
    <skos:definition xml:lang="en">Targeting users who have previously purchased or require a reason, incentive or nudge to purchase, e.g., you previously bought X, we recommend Y; 30% sale on now.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Behavior"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Region">
    <rdfs:label>Region</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their geographic region, i.e., a location within a country.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Geographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Reminder">
    <rdfs:label>Reminder</rdfs:label>
    <skos:definition xml:lang="en">Notifications that serve as a reminder or alert to the users or provide advice / tips.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Residence">
    <rdfs:label>Residence</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their type of residence, e.g., their house contains a garden.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Demographic"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Seasons">
    <rdfs:label>Seasons</rdfs:label>
    <skos:definition xml:lang="en">The notification will only be relevant in certain seasons (summer, winter, ...).</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Shopping">
    <rdfs:label>Shopping</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Social">
    <rdfs:label>Social</rdfs:label>
    <skos:definition xml:lang="en">Notifications that encourage the subscriber to engage with social tools e.g. share, like, comment, reply.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Sports">
    <rdfs:label>Sports</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Strategy">
    <rdfs:label>Strategy</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/TravelAndLocal">
    <rdfs:label>Travel And Local</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Unclear">
    <rdfs:label>Unclear</rdfs:label>
    <skos:definition xml:lang="en">The notification conveys some urgency (for example using words like 'now' or 'hurry up'), but from the content of the notification, it is not possible to identify how long it will remain relevant.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/VideoPlayersAndEditors">
    <rdfs:label>Video Players And Editors</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Watch">
    <rdfs:label>Watch</rdfs:label>
    <skos:definition xml:lang="en">Notifications that encourage the user to watch some content on the application.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/WatchApps">
    <rdfs:label>Watch Apps</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/WatchFaces">
    <rdfs:label>Watch Faces</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Weather">
    <rdfs:label>Weather</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Word">
    <rdfs:label>Word</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:Class>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/accountCreated">
    <rdfs:label>account created</rdfs:label>
    <skos:definition xml:lang="en">User created an account to use the application.</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/appName">
    <rdfs:label>app name</rdfs:label>
    <skos:definition xml:lang="en">The name of the application</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/canBeUsedToInfer">
    <rdfs:label>can be used to infer</rdfs:label>
    <skos:definition xml:lang="en">Indicates the type of personal data that can be inferred from the notification.</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/Personalisation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/hasAppType">
    <rdfs:label>has app type</rdfs:label>
    <skos:definition xml:lang="en">Indicates the application type</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/ApplicationType"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/hasCTA">
    <rdfs:label>has call-to-action</rdfs:label>
    <skos:definition xml:lang="en">Indicates the call to action of the notification</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/CallToAction"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/hasCampaignType">
    <rdfs:label>has campaign type</rdfs:label>
    <skos:definition xml:lang="en">Indicates the campaign type of the notification</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/hasTargetAudience">
    <rdfs:label>has target audience</rdfs:label>
    <skos:definition xml:lang="en">Indicates the target audience of the notification</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/TargetAudience"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/inAppPurchases">
    <rdfs:label>in app purchases</rdfs:label>
    <skos:definition xml:lang="en">Indicates if the application includes paid features/services</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/installDate">
    <rdfs:label>install date</rdfs:label>
    <skos:definition xml:lang="en">Indicates the date and time when the application was installed</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/isTimeSensitive">
    <rdfs:label>is time sensitive</rdfs:label>
    <skos:definition xml:lang="en">Indicates weather the notification is urgent, i.e. time-sensitive, or not.</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/notificationID">
    <rdfs:label>notification ID</rdfs:label>
    <skos:definition xml:lang="en">Unique identifier of the notification</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/notificationText">
    <rdfs:label>notification text</rdfs:label>
    <skos:definition xml:lang="en">Text content of the notification, including emojis</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/numberOfDownloads">
    <rdfs:label>number of downloads</rdfs:label>
    <skos:definition xml:lang="en">Indicates the number of times that the application was downloaded</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/numberOfReviews">
    <rdfs:label>number of reviews</rdfs:label>
    <skos:definition xml:lang="en">Indicates the number of reviews that the application has on the app store</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/pushedBy">
    <rdfs:label>pushed by</rdfs:label>
    <skos:definition xml:lang="en">Indicates the app that pushed the notification</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/Application"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/pushedOn">
    <rdfs:label>pushed on</rdfs:label>
    <skos:definition xml:lang="en">Date and time when the notification is pushed by the application</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/apn/remainsRelevant">
    <rdfs:label>remains relevant</rdfs:label>
    <skos:definition xml:lang="en">Indicates the urgency of the notification - the period of time when the notification remains relevant for the user</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/PushNotification"/>
    <rdfs:range rdf:resource="https://w3id.org/apn/Urgency"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/requiresAccount">
    <rdfs:label>requires account</rdfs:label>
    <skos:definition xml:lang="en">Indicates if the application requires the creation of an account to use</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/sizeInMB">
    <rdfs:label>size in MB</rdfs:label>
    <skos:definition xml:lang="en">Indicates the size of the application in megabytes</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://w3id.org/apn/stars">
    <rdfs:label>stars</rdfs:label>
    <skos:definition xml:lang="en">Indicates the mean number of stars given by the users to the application</skos:definition>
    <rdfs:domain rdf:resource="https://w3id.org/apn/Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:Class rdf:about="https://w3id.org/apn/Application">
    <rdfs:label>Application</rdfs:label>
    <skos:definition xml:lang="en">Computer program or software application designed to run on a mobile device such as a phone, tablet, or watch. Mobile applications often stand in contrast to desktop applications which are designed to run on desktop computers, and web applications which run in mobile web browsers rather than directly on the mobile device.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Geographic">
    <rdfs:label>Geographic</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their geographic whereabouts.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/TargetAudience"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CT_Promotional">
    <rdfs:label>Campaign Type: Promotional</rdfs:label>
    <skos:definition xml:lang="en">A campaign based on promotional offers for the users.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/CampaignType"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Psychographic">
    <rdfs:label>Psychographic</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their interests, personal motivations or lifestyle.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/TargetAudience"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Demographic">
    <rdfs:label>Demographic</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on quantifiable characteristics such as family status or job experience. </skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/TargetAudience"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Behavior">
    <rdfs:label>Behavior</rdfs:label>
    <skos:definition xml:lang="en">Targeting users based on their behavior, purchase intent and interaction with certain online services or products.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/apn/TargetAudience"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/TargetAudience">
    <rdfs:label>Target Audience</rdfs:label>
    <skos:definition xml:lang="en">Indicates the audience being targeted by the notification, based on their behavior or other demographic, geographic or psychographic characteristics.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/PushNotification">
    <skos:definition xml:lang="en">Push notifications are pop-up messages, sent by mobile or browser applications, that appear on the users’ devices. Subscribers can be anywhere and still receive these messages as long as they’re online or have their browsers running on their devices.</skos:definition>
    <rdfs:label>Push Notification</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Urgency">
    <rdfs:label>Urgency</rdfs:label>
    <skos:definition xml:lang="en">The urgency of the notification - if it contains information that is only relevant for a certain period of time and becomes redundant if that period of time passes (for example, a notification about Friday night is irrelevant if delivered on Saturday morning).</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CampaignType">
    <rdfs:label>Campaign Type</rdfs:label>
    <skos:definition xml:lang="en">Indicates the campaign type of the notification, based on the marketing strategy followed by a company to keeps its brand's visibility, i.e., to mantain and increase the number of notification subscribers.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/CallToAction">
    <rdfs:label>Call To Action</rdfs:label>
    <skos:definition xml:lang="en">The call-to-action of the notification - for example, 'Anne sent you a message on App X' is encouraging the user to interact with the app to communicate with other people.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/Personalisation">
    <rdfs:label>Personalisation</rdfs:label>
    <skos:definition xml:lang="en">Indicates personal information that can be derived or inferred from the notification.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/apn/ApplicationType">
    <rdfs:label>Application Type</rdfs:label>
    <skos:definition xml:lang="en"></skos:definition>
  </owl:Class>

</rdf:RDF>
